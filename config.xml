<?xml version='1.0' encoding='utf-8'?>
<widget id="kr.go.juso.smartKais" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>스마트Kais</name>
    <description>
    </description>
    <author email="" href="">
          도로명주소 정보화 사업단
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>


    <feature name="MKaisvPlugin">
        <param name="android-package" value="kr.go.juso.smartKais.plugin.MKaisvPlugin" />
    </feature>
    <preference name="Orientation" value="portrait" />

</widget>
